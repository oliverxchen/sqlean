        ),
        Token("FROM", "from"),
        Tree(
            "from_clause",
            [
                Token("TABLE_NAME", "table"),
                Tree(
                    "from_modifier",
                    [
                        Token("WHERE", "where"),
                        Tree(
                            "where_list",
                            [
                                Tree(
                                    "first_where_item",
                                    [
