select field from `project.dataset.table`

---

Tree(
    "query_expr",
    [
        Token("SELECT", "select"),
        Tree(
            "select_list",
            [Tree("select_item_unaliased", [Token("FIELD_NAME", "field")])],
        ),
        Token("FROM", "from"),
        Tree(
            "from_clause",
            [
                Tree(
                    "explicit_table_name",
                    [
                        Token("PROJECT_ID", "project"),
                        Token("DATASET_ID", "dataset"),
                        Token("TABLE_ID", "table"),
                    ],
                )
            ],
        ),
    ],
)
