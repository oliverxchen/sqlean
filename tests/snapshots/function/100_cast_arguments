select
	cast(a as int) as alias,
	cast(b as int64) alias_2,
	cast(c as string),
	cast(d as numeric),
	cast(e as bytes),
	cast(f as bytes(3)),
	cast(g as decimal),
	cast(h as float64),
from table

---

SELECT
    CAST(a AS INT) AS alias,
    CAST(b AS INT64) AS alias_2,
    CAST(c AS STRING),
    CAST(d AS NUMERIC),
    CAST(e AS BYTES),
    CAST(f AS BYTES(3)),
    CAST(g AS DECIMAL),
    CAST(h AS FLOAT64),
FROM
    table

