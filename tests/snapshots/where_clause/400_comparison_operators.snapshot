select
  field
from
  table
where
  a > "b"
  and b >= 1
  and c < d
  and a
  and (e = f or g)
  and d <= e
  and e = f
  and f != g
  and g <> h
  and h like "%foo%"
  and i not like "%foo%"
  and j in (1, 2, 3)
  and k not in ("a", "b", "c")
  and el between 1 and 2
  and m not between "2020-01-01" and "2022-02-22"

