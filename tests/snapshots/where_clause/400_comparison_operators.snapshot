select
  field
from
  table
where
  a > "b"
  and b >= 1
  and c < d
  and a
  and (e = f or g)
  and d <= e
  and e = f
  and f != g
  and g <> h
  and h like "%foo%"
  and i not like "%foo%"
  and j in (1, 2, 3)
  and k not in ("a", "b", "c")

