select field from (select field, field2 from table)

---

SELECT
    field,
FROM
    (
        SELECT
            field,
            field2,
        FROM
            table
    )

---

Tree(
    "query_file",
    [
        Tree(
            "select_expr",
            [
                Tree("select_type", [Token("SELECT", "select")]),
                Tree("select_list", [Tree("select_item_unaliased", [Token("CNAME", "field")])]),
                Token("FROM", "from"),
                Tree(
                    "from_clause",
                    [
                        Tree(
                            "sub_query_expr",
                            [
                                Token("LPAR", "("),
                                Tree(
                                    "select_expr",
                                    [
                                        Tree("select_type", [Token("SELECT", "select")]),
                                        Tree(
                                            "select_list",
                                            [
                                                Tree("select_item_unaliased", [Token("CNAME", "field")]),
                                                Tree("select_item_unaliased", [Token("CNAME", "field2")]),
                                            ],
                                        ),
                                        Token("FROM", "from"),
                                        Tree("from_clause", [Tree("simple_table_name", [Token("CNAME", "table")])]),
                                    ],
                                ),
                                Token("RPAR", ")"),
                            ],
                        )
                    ],
                ),
            ],
        )
    ],
)
