select field from table t

---

SELECT
    field,
FROM
    table AS t

---

Tree(
    "query_file",
    [
        Tree(
            "query_expr",
            [
                Tree("select_statement", [Token("SELECT", "select")]),
                Tree("select_list", [Tree("select_item_unaliased", [Token("CNAME", "field")])]),
                Token("FROM", "from"),
                Tree(
                    "from_clause",
                    [
                        Tree(
                            "table_item_aliased",
                            [Tree("simple_table_name", [Token("CNAME", "table")]), Token("CNAME", "t")],
                        )
                    ],
                ),
            ],
        )
    ],
)
