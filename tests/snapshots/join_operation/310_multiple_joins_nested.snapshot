with t as (select * from
	table_a join table_b using(id)
	cross join table_c
	right join table_d on table_c.id = table_d.id
)
select * from t

---

WITH

t AS (
    SELECT
        *,
    FROM
        table_a
    INNER JOIN
        table_b
    USING (id)
    CROSS JOIN
        table_c
    RIGHT OUTER JOIN
        table_d
    ON
        table_c.id = table_d.id
)

SELECT
    *,
FROM
    t

