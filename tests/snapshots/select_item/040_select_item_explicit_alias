select field as new_field_name from table

---

SELECT
    field AS new_field_name,
FROM
    table

---

Tree(
    "query_file",
    [
        Tree(
            "query_expr",
            [
                Tree("select_statement", [Token("SELECT", "select")]),
                Tree(
                    "select_list",
                    [Tree("select_item_aliased", [Token("CNAME", "field"), Token("CNAME", "new_field_name")])],
                ),
                Token("FROM", "from"),
                Tree("from_clause", [Tree("simple_table_name", [Token("CNAME", "table")])]),
            ],
        )
    ],
)
