select table.field from table

---

SELECT
    table.field,
FROM
    table

---

Tree(
    "query_file",
    [
        Tree(
            "query_expr",
            [
                Tree("select_statement", [Token("SELECT", "select")]),
                Tree(
                    "select_list",
                    [
                        Tree(
                            "select_item_unaliased",
                            [Tree("table_referenced_field", [Token("CNAME", "table"), Token("CNAME", "field")])],
                        )
                    ],
                ),
                Token("FROM", "from"),
                Tree("from_clause", [Tree("simple_table_name", [Token("CNAME", "table")])]),
            ],
        )
    ],
)
