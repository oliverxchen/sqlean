?start: query

!query: SELECT select_list FROM from_clause

SELECT: "SELECT"i
FROM: "FROM"i

from_clause: table_id
//  (join_type table_name (using_clause)*)*

table_id: "`" PROJECT_NAME  "." DATASET_NAME "." TABLE_NAME "`"
    | "`" PROJECT_NAME  "`.`" DATASET_NAME "`.`" TABLE_NAME "`"
    | "(" query ")"
    | CNAME

PROJECT_NAME: CNAME
DATASET_NAME: CNAME
TABLE_NAME: CNAME

%import select.select_list
%import common.WS
%import common.CNAME
%ignore WS
