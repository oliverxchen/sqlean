// This should be parsed for real instead of using regex so that we can
// know the contents of these jinja bits.
CONFIG: /{{\s*?config\([^}]*\)\s*?}}/s
MACRO: /{{.*?\([^}]*\)\s*?}}/s
DBT_REF: /{{\s*?ref\([^}]*\)\s*?}}/s
DBT_SOURCE: /{{\s*?source\([^}]*\)\s*?}}/s
dbt_table_name: DBT_SOURCE
    | DBT_REF

%import common.CNAME
